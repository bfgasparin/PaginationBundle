<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="mqm_pagination.query_pagination.class">MQM\Bundle\PaginationBundle\Entity\QueryPagination</parameter>  
    </parameters>

    <services>
        <service id="mqm_pagination.query_pagination" class="%mqm_pagination.query_pagination.class%" public="true" scope="request">
            <argument type="service" id="mqm_pagination.pagination" />
        </service>
    </services>
    
</container>
